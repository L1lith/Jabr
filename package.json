{
  "name": "jabr",
  "version": "0.3.6",
  "description": "No-nonsense State Management",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && cross-env NODE_ENV=production babel source -d dist",
    "dev": "rm -rf dist && mkdir dist && babel source -d dist --watch",
    "test": "mocha \"tests/allTests/**/*.js\"",
    "lint": "npx eslint --fix source",
    "pretty": "prettier --write \"./**/*.js\"",
    "prepub": "npm run pretty && npm run lint && npm run build && npm run test && cp package.json dist && cp LICENSE.md dist && cp README.md dist && node ./cleanupPackage.js",
    "pub": "cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/L1lith/Jabr.git"
  },
  "keywords": [
    "state",
    "react",
    "data",
    "management",
    "proxy",
    "json"
  ],
  "author": "L1lith",
  "license": "ESMV",
  "bugs": {
    "url": "https://github.com/L1lith/Jabr/issues"
  },
  "homepage": "https://github.com/L1lith/Jabr#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.3",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint-plugin-mocha": "^10.0.4",
    "globals": "^16.3.0",
    "microbundle": "^0.13.0",
    "mocha": "^11.7.1",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "auto-bind": "^1.2.1",
    "sandhands": "^1.8.9",
    "tiny-emitter": "^2.1.0"
  }
}
